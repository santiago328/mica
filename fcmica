from datetime import date

def es_cumpleaÃ±os(fecha_str):
  """Comprueba si la fecha dada es el cumpleaÃ±os del usuario."""
  fecha_hoy = date.today()
  try:
    fecha_dada = date.fromisoformat(fecha_str)
    return (fecha_dada.day == fecha_hoy.day and
            fecha_dada.month == fecha_hoy.month)
  except ValueError:
    return False

fecha_entrada = input("Introduce tu fecha de cumpleaÃ±os (YYYY-MM-DD): ")
if es_cumpleaÃ±os(fecha_entrada):
  print("ğŸ‰ Â¡Feliz cumpleaÃ±os! ğŸ‰ ğŸ‚ğŸ¥³")
else:
  print("Hoy no es tu cumpleaÃ±os.")
